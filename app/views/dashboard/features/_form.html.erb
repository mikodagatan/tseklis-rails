<%= simple_form_for [:dashboard, resource], html: { multipart: true } do |f| %>
    <%= f.input :name %>
    <%= f.input :photo %>
    <%= f.input :video, as: :file %>
    <%= f.input :short_description %>
    <%= f.input :long_description %>
    <%= f.input :active %>
  <div class="space"></div>
  <%= f.button :submit, class: "button primary" %>
<% end %>
