<tr>
	<td><%= leave_request.title %></td>
	<td><%= @leave_types.find(leave_request.leave_type_id).name %></td>
	<td><%= leave_request.start_date %> <%= leave_request.start_time.strftime("%I:%M%p") %></td>
	<td><%= leave_request.end_date %> <%= leave_request.end_time.strftime("%I:%M%p") %></td>


	<td>
		<%= link_to 'Edit', edit_user_employment_leave_request_url(current_user, employment, leave_request), class: 'button' %>
	</td>
</tr>