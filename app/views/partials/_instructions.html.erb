<div id="instructions">
  <div class="row instructions align-middle">
    <div class="columns text-center">
      <div class="row">
        <div class="columns text-center">
          <h4>Company Setup</h4>
        </div>
      </div>
      <div class="space2"></div>
      <div class="row">
        <div class="columns">
          <div class="row medium-up-1 small-up-1">
            <div class="columns">
              <%= render 'partials/create_company' %>
            </div>
            <div class="columns">
              <%= render 'partials/company_edit' %>
            </div>
          </div>
        </div>
        <div class="columns">
          <div class="row medium-up-1 small-up-1">
            <div class="columns">
              <%= render 'partials/connect_company' %>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>
